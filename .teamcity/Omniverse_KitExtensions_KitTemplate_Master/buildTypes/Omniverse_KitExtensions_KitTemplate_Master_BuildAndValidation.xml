<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="4968debe-af90-4f51-9230-5d69e0b7bd15" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>build and validation</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <parameters />
    <build-runners />
    <vcs-settings>
      <vcs-entry-ref root-id="Omniverse_KitExtensions_KitTemplate_GitlabMasterOmniverseKitExtensionsKitTemplate" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter"><![CDATA[+:<default>
+:merge-requests*]]></param>
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <dependencies>
      <depend-on sourceBuildTypeId="Omniverse_KitExtensions_KitTemplate_Master_Building_GenerateBuildNumber">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Omniverse_KitExtensions_KitTemplate_Master_Building_BuildWindowsX8664">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Omniverse_KitExtensions_KitTemplate_Master_Building_BuildLinuxX8664">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Omniverse_KitExtensions_KitTemplate_Master_Testing_TestWindowsX8664">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Omniverse_KitExtensions_KitTemplate_Master_Testing_TestLinuxX8664">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

