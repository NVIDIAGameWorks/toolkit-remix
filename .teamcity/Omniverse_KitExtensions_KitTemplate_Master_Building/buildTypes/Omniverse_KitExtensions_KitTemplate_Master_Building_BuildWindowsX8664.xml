<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="62f7ecc1-7a0d-432f-ae0c-97d9cc2b33b3" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>build (windows-x86_64)</name>
  <description />
  <settings>
    <options>
      <option name="buildNumberPattern" value="%dep.Omniverse_KitExtensions_KitTemplate_Master_Building_GenerateBuildNumber.build.number%" />
      <option name="cleanBuild" value="true" />
      <option name="executionTimeoutMin" value="30" />
    </options>
    <parameters>
      <param name="env.BUILD_NUMBER" value="%build.number%" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_2481" name="" type="simpleRunner">
        <parameters>
          <param name="org.jfrog.artifactory.selectedDeployableServer.downloadSpecSource" value="Job configuration" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.uploadSpecSource" value="Job configuration" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.useSpecs" value="false" />
          <param name="script.content" value="tools/ci/building/build-windows-x86_64/step.bat" />
