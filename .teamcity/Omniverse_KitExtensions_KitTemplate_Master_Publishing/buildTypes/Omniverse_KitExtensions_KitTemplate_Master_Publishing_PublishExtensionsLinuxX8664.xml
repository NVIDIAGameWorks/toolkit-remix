<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="4d55be8c-8a7d-4e7c-914d-52a492be1f23" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>publish extensions (linux-x86_64)</name>
  <description />
  <settings>
    <options>
      <option name="buildNumberPattern" value="%dep.Omniverse_KitExtensions_KitTemplate_Master_Building_GenerateBuildNumber.build.number%" />
      <option name="cleanBuild" value="true" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_4583" name="" type="simpleRunner">
        <parameters>
          <param name="org.jfrog.artifactory.selectedDeployableServer.downloadSpecSource" value="Job configuration" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.uploadSpecSource" value="Job configuration" />
          <param name="org.jfrog.artifactory.selectedDeployableServer.useSpecs" value="false" />
          <param name="script.content" value="tools/ci/publishing/publish-extensions-linux-x86_64/step.sh" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Omniverse_KitExtensions_KitTemplate_GitlabMasterOmniverseKitExtensionsKitTemplate" />
    </vcs-settings>
    <requirements>
      <not-exists id="RQ_4207" name="system.feature.nvidia.gpu.driver.major" />
      <contains id="RQ_4206" name="teamcity.agent.jvm.os.name" value="Linux" />
    </requirements>
    <build-triggers />
    <build-extensions>
      <extension id="jetbrains.agent.free.space" type="jetbrains.agent.free.space">
        <parameters>
          <param name="free-space-work" value="3gb" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1679" sourceBuildTypeId="Omniverse_KitExtensions_KitTemplate_Master_Building_BuildLinuxX8664" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="*.7z =&gt; _build/packages&#xD;&#xA;" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Omniverse_KitExtensions_KitTemplate_Master_Building_BuildLinuxX8664">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="run-build-if-dependency-failed-to-start" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

