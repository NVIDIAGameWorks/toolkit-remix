    "version": "0.2.0",
    "configurations": [
        {
            "name": "Process: Attach (windows-x86_64)",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
		{
            "name": "Python: Attach (windows-x86_64/linux-x86_64)",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}",
            "port": 3000,
            "host": "localhost",
            "subProcess": true,
            "runtimeArgs": [
                "--preserve-symlinks",
                "--preserve-symlinks-main"
            ]
        },
        {
            "name": "omni.app.new_exts_demo (debug, windows-x86_64)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/_build/windows-x86_64/debug/kit/kit.exe",
            "args": ["${workspaceFolder}/_build/windows-x86_64/debug/apps/omni.app.new_exts_demo.kit"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "omni.app.new_exts_demo (release, windows-x86_64)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/_build/windows-x86_64/release/kit/kit.exe",
            "args": ["${workspaceFolder}/_build/windows-x86_64/release/apps/omni.app.new_exts_demo.kit"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
