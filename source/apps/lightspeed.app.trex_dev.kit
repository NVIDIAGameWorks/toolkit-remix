[package]
version = "2024.3.1"
category = "Application"
title = "NVIDIA RTX Remix Dev"
description = "Dev app that regroups all extensions shared between all NVIDIA RTX Remix experiences"
authors = ["Damien Bataille <dbataille@nvidia.com>"]
repository = "https://gitlab-master.nvidia.com/lightspeedrtx/lightspeed-kit"
keywords = ["lightspeed", "rtx", "remix", "app", "dev"]
changelog = """
# NVIDIA RTX Remix

## [2022.0.0] - 2022-06-14
- Publishing first version.
"""
readme = """
# NVIDIA RTX Remix Experience

This dev experience regroup all extensions shared between all NVIDIA RTX Remix experiences
"""

[dependencies]
"lightspeed.app.trex" = {}
# "omni.kit.profile_python" = {}
"omni.kit.menu.file" = {}
"omni.kit.profiler.tracy" = {}
"omni.kit.profiler.window" = {}
"omni.kit.window.extensions" = {}
"omni.kit.window.script_editor" = {}

[settings]
"@import@" = [
	"./lightspeed.app.trex.app.settings.toml",
	"./lightspeed.app.rendering.settings.toml",
]

# Basic Kit App
################################
app.name = "RTX Remix Dev"

[settings.app.window]
title = "NVIDIA RTX Remix Dev"

[settings.exts."omni.kit.window.modifier.titlebar"]
titleFormatString = "  NVIDIA RTX Remix Dev {verKey:/app/version,font_color=0x909090,font_size=16} {separator} {file, board=true}"

[settings.exts."lightspeed.trex.app.setup"]
hide_menu = false

[[test]]
args = [
	"--no-assert-dialog",
	"--/rtx/materialDb/syncLoads=true",
	"--/omni.kit.plugin/syncUsdLoads=true",
	"--/rtx/hydra/materialSyncLoads=true",
	"--/renderer/multiGpu/autoEnable=false",
	"--/app/hydraEngine/skipStatusBarIdle=true",
	"--/app/asyncRendering=false",
	"--/renderer/debug/validation/enabled=false",
	# invoking an RTX renderer, while a Remix renderer is present, causes a crash
	# see lightspeed.hydra.remix.core\config\extension.toml
	# "--/renderer/enabled='rtx'",
    "--no-window"
]

dependencies = [
    "omni.kit.ui_test",
    "omni.kit.test_suite.helpers"
]

stdoutFailPatterns.exclude = [
    "*OpenGL Interop is not available. Upgrade your driver to latest for this feature.*",
    "*Cannot use omni.hydra.pxr without OpenGL interop*"
]

# We can run all extension tests inside of an app. In practice many of them failing and crashing, that requires time
# investment to fix. Add one extension, to keep pipeline working:
pythonTests.include = [
]
pythonTests.exclude = [
]
timeout = 600



